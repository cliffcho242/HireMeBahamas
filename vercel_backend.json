{
  "version": 2,
  "name": "hiremebahamas-backend",
  "functions": {
    "final_backend.py": {
      "runtime": "python3.12",
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "installCommand": "pip install --upgrade pip && pip install --only-binary=:all: -r requirements.txt",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/final_backend.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
}
