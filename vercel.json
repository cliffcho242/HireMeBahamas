{
  "version": 2,
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@1.3.1",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}
